{
    "env": {
        "node": true,
        "browser": true
    },
    "rules": {
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs"],
        "camelcase": 0,
        "consistent-this": 2,
        "default-case": 2,
        "eol-last": 0,
        "func-style": [2, "declaration"],
        "guard-for-in": 2,
        "handle-callback-err": 0,
        "no-catch-shadow": 2,
        "no-div-regex": 2,
        "no-floating-decimal": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-extra-parens": 2,
        "no-lonely-if": 2,
        "no-self-compare": 2,
        "no-trailing-spaces": 2,
        "no-undefined": 2,
        "no-underscore-dangle": 0,
        "quotes": [2, "single"],
        "radix": 2,
        "space-after-keywords": 2,
        "space-unary-word-ops": 2,
        "valid-jsdoc": [2, {
            "requireReturn": false
        }],
        "wrap-iife": 2
    },
    "globals": {
        "afterEach": false,
        "angular": false,
        "beforeEach": false,
        "browser": false,
        "by": false,
        "element": false,
        "expect": false,
        "describe": false,
        "jasmine": false,
        "inject": false,
        "it": false,
        "spyOn": false
    }
}